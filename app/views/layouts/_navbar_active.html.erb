<script type="text/javascript">
$(document).ready(function(){
	var $portfolio = $("#nav_<%= @header %>");
	if(!$portfolio.hasClass('active')){
		$portfolio.addClass('active');
	}
	var $portfolio = $("#nav_left_<%= @header %>");
	if(!$portfolio.hasClass('current')){
		$portfolio.addClass('current');
	}
})
</script>